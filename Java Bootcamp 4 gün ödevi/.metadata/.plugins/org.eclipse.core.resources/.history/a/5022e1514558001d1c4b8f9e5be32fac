package Kodlama.io.Devs.dataAccess.concretes;

import java.util.List;

import Kodlama.io.Devs.dataAccess.abstracts.IRepository;
import Kodlama.io.Devs.entities.Language;

public class InMemoryRepository implements IRepository{
	
	
	private List<Language> languages;
	
	public InMemoryRepository(List<Language> languages) {
		this.languages = languages;
	}
	
	
	public InMemoryRepository() {
		
		
		languages.add(new Language(1,"Python"));
		languages.add(new Language(2,"Java"));
		languages.add(new Language(3,"Javascript"));
		
	}
	

	@Override
	public List<Language> getAll() {
		return null;
	}

	@Override
	public List<Language> getId() {
		
		return null;
	}

	@Override
	public List<Language> add() {
		
		return null;
	}

	@Override
	public List<Language> update() {
		
		return null;
	}

	@Override
	public List<Language> delete() {
		
		return null;
	}
	
	

}
